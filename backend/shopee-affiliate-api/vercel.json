{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend-react/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "index.js" },
    { "src": "^/static/(.*)", "dest": "frontend-react/build/static/$1" },
    { "src": "^/(manifest\\.json|favicon\\.ico|logo\\d+\\.\\w+|robots\\.txt)", "dest": "frontend-react/build/$1" },
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "ignoreCommand": "true"
}
